{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend side of the project",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ./src/index.ts",
    "start": "node ./dist/index.js",
    "test": "echo \"Error: no test specified\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "db:createMigration": "cd ./src/database/migrations && typeorm migration:create",
    "db:migrationRun": "npm run build && npx typeorm-ts-node-commonjs migration:run -d src/database/DataSource.ts",
    "db:migrationRevert": "typeorm-ts-node-commonjs migration:revert -d src/database/DataSource.ts",
    "db:migrationRunFake": "npm run build && npx typeorm-ts-node-commonjs migration:run -d src/database/DataSource.ts --fake",
    "db:migrationRevertFake": "typeorm-ts-node-commonjs migration:revert -d src/database/DataSource.ts --fake"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.1",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17",
    "winston": "^3.10.0"
  }
}
